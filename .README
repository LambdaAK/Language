expression ::= term addop expression
    | term


term ::= factor mulop term
    | factor


factor ::= <number>
    | ( expression )
    | - factor
    | factor powop factor
    | factor unop



addop ::= +
    | -


mulop ::= *
    | /
    | %

power ::= ^


unop ::= !

------------------
------------------
------------------
------------------

function ::= <"print">

functioncall ::= function ( functionargs )
    | function ( )

functionargs ::= functionarg [, functionarg]*

functionarg ::= expression




